	<!-- Excluding products from SEO -->
	{% if handle contains 'birthday-gift-2020' %}
	<meta name="robots" content="noindex">
	{% endif %}
	<!-- END Excluding products from SEO -->


</head>

<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }} flexbox-wrapper{% unless settings.animations_enabled %} animations-disabled{% endunless %}">

	{% if template != 'cart' %}
		{% include 'cart-drawer' %}
	{% endif %}

	<div id="main-body">

		<div id="slideout-overlay"></div>

		{% section 'announcement-bar' %}
		{% section 'header' %}
    {% section 'text-adverts' %}

		<div id="main" role="main">

			{% unless template == "index" or template == "collection" %}
				<div class="container">
			{% endunless %}

			{{ content_for_layout }}
			<limespot></limespot>

			{% unless template == "index" or template == "collection" %}
				</div>
			{% endunless %}

			<!-- Product page Reviews -->
			{% if template == "product" %}
				{% section 'logo-bar' %}
				{% section 'product-template-reviews' %}
				{% section 'product-template-bottom' %}
			{% endif %}
			<!-- END Product page Reviews -->

		</div>

		{% section 'footer' %}
		{% include 'homepage-scripts' %}

	</div>

	{% include 'popup' %}

	{% if settings.show_multiple_currencies %}
		{% include 'currencies' %}
	{% endif %}

	<script>
		// Translations that will be used in javascript section handling
		window.theme = {
        strings: {
            addToCart: {{ 'products.product.add_to_cart' | t | json }},
            added: {{ 'products.product.added' | t | json }},
            soldOut: {{ 'products.product.sold_out' | t | json }},
            unavailable: {{ 'products.product.unavailable' | t | json }},
            make_a_selection: {{ 'products.product.make_a_selection' | t | json }},
            showMore: {{ 'general.filters.show_more' | t | json }},
            showLess: {{ 'general.filters.show_less' | t | json }}
        },
        moneyFormat: {{ shop.money_with_currency_format | json }}
    };
	</script>



	{% comment %} Conditional statement  added by Smart Wishlist to fix search issue on Wishlist page on June 13, 2018 {% endcomment %}
	{% unless template %}
	<script>
	  $(document).ready(function(){

	      $(".search-show").off('click').on('click', function(){
	          $("#top-search-wrapper").toggleClass('expanded');
	          $("#top-search-wrapper #search_text").focus();
	      });

	      if($(window).width() < 767){
	          $('.mobile-menu-close').trigger('click');
	      }
	  });
	</script>
	{% endunless %}




