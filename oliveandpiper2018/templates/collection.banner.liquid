<!-- /templates/collection.banner.liquid -->
<script> 
{% if collection.products_count <= 20 %} 
LimeSpot.PageInfo = { Type: "/collection20/" }
{% else collection.handle === "sale" %}
LimeSpot.PageInfo = { Type: "/sale/" }
{% else collection.handle === "new-arrivals" %}
LimeSpot.PageInfo = { Type: "/new-arrivals/" }
{% else collection.handle === "back-in-stock" %}
LimeSpot.PageInfo = { Type: "/back-in-stock/" }
{% else collection.handle === "pre-order" %}
LimeSpot.PageInfo = { Type: "/pre-order/" }
{% endif %} 
</script> 

{% comment %}
  The contents of the collection.banner.liquid template can be found in /sections/collection-template.liquid
{% endcomment %}

{% section 'collection-template-banner' %}
